<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>


<BorderPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="museumtimetracking.gui.views.root.statistics.ROIOverview.ROIGmHoursViewController">
   <center>
      <AnchorPane id="AnchorPane">
         <children>
            <PieChart fx:id="chartPie" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0" />
         </children>
      </AnchorPane>
   </center>
   <right>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <HBox layoutX="-14.0" layoutY="28.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0">
               <children>
                  <TextField fx:id="txtSearchBar" />
                  <ComboBox fx:id="cmbGuilds" onAction="#selectGuild" prefWidth="150.0" />
               </children>
            </HBox>
         </children>
      </AnchorPane>
   </right>
</BorderPane>
