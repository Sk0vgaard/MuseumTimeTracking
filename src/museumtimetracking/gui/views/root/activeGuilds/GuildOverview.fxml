<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="501.0" prefWidth="673.0" stylesheets="@../../css/Style.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="museumtimetracking.gui.views.root.activeGuilds.GuildOverviewController">
   <children>
      <TableView fx:id="tableGuild" layoutX="391.0" layoutY="14.0" onMouseClicked="#handleSelectGuild" prefHeight="427.0" prefWidth="324.0" AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="391.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0">
         <columns>
            <TableColumn fx:id="clmGuildName" prefWidth="182.0" text="Laug" />
            <TableColumn fx:id="clmGuildDescription" prefWidth="234.0" text="Beskrivelse" />
            <TableColumn fx:id="clmGM" prefWidth="75.0" text="Tovholder" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Pane fx:id="newGuildPane" prefHeight="176.0" prefWidth="350.0" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="14.0">
         <children>
            <VBox layoutY="14.0" spacing="15.0">
               <children>
                  <Label id="h1" text="Nyt Laug:">
                     <font>
                        <Font name="System Bold" size="21.0" />
                     </font>
                  </Label>
                  <Label text="Laugets Tovholder:">
                     <font>
                        <Font name="System Bold" size="21.0" />
                     </font>
                  </Label>
                           
               </children>
            </VBox>
            <VBox layoutX="191.0" layoutY="14.0" spacing="15.0">
               <children>
                  <JFXTextField fx:id="txtGuildName" promptText="Navn på Laug">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </JFXTextField>
                  <JFXComboBox fx:id="cmbGuildManager" onAction="#handleSelectGuildManager" prefHeight="25.0" prefWidth="156.0" promptText="Vælg Tovholder" />

               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" />
         </padding>
      </Pane>
      <ButtonBar fx:id="guildOptions" layoutX="391.0" layoutY="448.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="54.0" prefWidth="289.0" AnchorPane.bottomAnchor="-1.0" AnchorPane.rightAnchor="0.0">
        <buttons>
            <Button mnemonicParsing="false" onAction="#handleArchiveGuid" prefHeight="22.0" prefWidth="120.0" text="Arkiver Laug" />
            <Button mnemonicParsing="false" onAction="#handleDeleteGuid" prefHeight="22.0" prefWidth="89.0" text="Slet Laug" />
            <Button maxWidth="140.0" minWidth="140.0" mnemonicParsing="false" onAction="#handleEditGuild" prefWidth="140.0" text="Rediger" />
        </buttons>
      </ButtonBar>
      <Button layoutX="13.0" layoutY="448.0" mnemonicParsing="false" onAction="#handleAddGuild" prefHeight="9.0" prefWidth="84.0" stylesheets="@../../css/Style.css" text="Tilføj" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="14.0" />
      <TextArea fx:id="txtDescription" layoutX="14.0" layoutY="119.0" prefHeight="227.0" prefWidth="350.0" promptText="Beskrivelse..." AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="14.0" />
      <Label layoutX="14.0" layoutY="186.0" text="Laug beskrivelse:" AnchorPane.bottomAnchor="289.0" AnchorPane.leftAnchor="14.0">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
